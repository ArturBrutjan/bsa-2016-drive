@{
    ViewBag.Title = "Home Page";
}
<div id="wrapper">
    <div ng-include="'Scripts/App/Menu/Menu.html'" class="include-index" id="sidebar-wrapper"></div>
    <div style="padding: 5px;  margin-bottom: -20px; ">
        <button href="#menu-toggle" class="btn btn-custom menu-toggle btn-md" style="margin-top: -10px">
            <i class="fa fa-bars fa-2x" aria-hidden="true"></i>
        </button>
    </div>
    <div ng-view class="view-index" id="page-content-wrapper"></div>
</div>
<script type="text/javascript">
    $(".menu-toggle").click(function (e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
</script>
